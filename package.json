{
  "dependencies": {
    "axios": "^0.27.2",
    "form-data": "^4.0.0",
    "md5": "^2.3.0",
    "md5-file": "^5.0.0"
  },
  "description": "Rapidgator api client for the v2 api",
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/md5": "^2.3.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "nock": "^13.3.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-mocha": "^10.0.0",
    "tslib": "^2.5.0",
    "tsup": "^6.5.0",
    "type-fest": "^3.10.0",
    "typescript": "^5.0.4"
  },
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "engines": {
    "node": ">=16"
  },
  "author": {
    "name": "pfftdammitchris",
    "email": "pfftdammitchris@gmail.com",
    "url": "https://github.com/pfftdammitchris"
  },
  "displayName": "rapidgatorv2",
  "homepage": "https://github.com/pfftdammitchris",
  "keywords": [
    "rapidgator",
    "rapidgatorv2"
  ],
  "readme": "README.md",
  "eslintConfig": {
    "env": {
      "es2020": true,
      "mocha": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "root": true,
    "rules": {
      "import/default": 2,
      "import/export": 2,
      "import/named": 2,
      "import/namespace": 2,
      "import/no-unresolved": [
        2,
        {
          "amd": true,
          "commonjs": true
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules",
    "dist"
  ],
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/pfftdammitchris/rapidgatorv2/issues"
  },
  "repository": {
    "url": "https://github.com/pfftdammitchris/rapidgatorv2.git"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "name": "rapidgatorv2",
  "nodemonConfig": {
    "ignore": [
      "*.json"
    ]
  },
  "packageManager": "pnpm@7.12.1",
  "prettier": {
    "arrowParens": "always",
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "tsup",
    "start": "tsup --watch",
    "test": "cross-env NODE_ENV=test ts-mocha -p tsconfig.test.json --extensions ts --file src/__tests__/setup.ts --watch --watch-files \"src/**/*\" \"src/**/*.test.*\" --paths"
  },
  "types": "./dist/index.d.ts",
  "version": "0.0.1",
  "workspaces": []
}
